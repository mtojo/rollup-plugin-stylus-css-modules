{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true,
    "node": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "rules": {
    "array-bracket-spacing": [2, "never"],
    "arrow-parens": 2,
    "arrow-spacing": 2,
    "brace-style": [2, "1tbs", {"allowSingleLine": false}],
    "comma-dangle": 0,
    "comma-spacing": 2,
    "comma-style": [2, "last"],
    "computed-property-spacing": [2, "never"],
    "consistent-return": 0,
    "consistent-this": [2, "DO NOT USE"],
    "constructor-super": 2,
    "curly": 0,
    "default-case": 2,
    "dot-notation": 0,
    "eqeqeq": 2,
    "generator-star-spacing": 2,
    "indent": [2, 2],
    "jsx-quotes": 2,
    "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
    "max-depth": [2, 3],
    "max-len": [2, 100, 2],
    "max-params": [2, 5],
    "new-cap": 0,
    "new-parens": 0,
    "no-alert": 1,
    "no-class-assign": 2,
    "no-cond-assign": 0,
    "no-console": 0,
    "no-const-assign": 2,
    "no-constant-condition": 1,
    "no-debugger": 1,
    "no-dupe-class-members": 2,
    "no-else-return": 2,
    "no-empty": 0,
    "no-empty-pattern": 2,
    "no-fallthrough": 0,
    "no-lonely-if": 2,
    "no-loop-func": 0,
    "no-multi-spaces": 2,
    "no-multiple-empty-lines": [1, {"max": 1}],
    "no-nested-ternary": 2,
    "no-new-require": 2,
    "no-process-exit": 0,
    "no-regex-spaces": 0,
    "no-return-assign": 0,
    "no-self-compare": 2,
    "no-shadow": 0,
    "no-spaced-func": 2,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unreachable": 0,
    "no-unused-expressions": 0,
    "no-use-before-define": 0,
    "no-var": 2,
    "no-void": 2,
    "object-curly-spacing": [2, "never"],
    "object-shorthand": 2,
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prefer-reflect": 2,
    "prefer-spread": 2,
    "prefer-template": 2,
    "quotes": [2, "single"],
    "require-yield": 2,
    "semi": [2, "always"],
    "space-after-keywords": [2, "always"],
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, "never"],
    "space-return-throw-case": 2,
    "strict": 0,
    "valid-jsdoc": [2, {"requireReturn": false}]
  }
}
